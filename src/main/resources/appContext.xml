<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="user" class="com.company.quiz.inject.spring_example.User" scope="prototype">
        <property name="address" ref="address"/>
    </bean>

    <bean id="address" class="com.company.quiz.inject.spring_example.Address" scope="prototype">
        <constructor-arg name="street" value="Green"/>
        <constructor-arg name="houseNum" value="123"/>
    </bean>
</beans>